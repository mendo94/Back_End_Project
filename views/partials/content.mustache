<section class="content-btn-container">
  <button id="sidebarCollapse" type="button"
    class="content-btn btn bg-light text-black-50 rounded-pill text-uppercase font-weight-bold px-4 mb-4">Create
    New
    Room</button>
  <a href="/homepage/create-box">
    <button id="sidebarCollapse" type="button"
      class="content-btn btn bg-light text-black-50 rounded-pill text-uppercase font-weight-bold px-4 mb-4">Create a
      new
      box</button>
  </a>
</section>
<!--viewing containers -->
<main>


  <div id="box-position-holder">
    <div class="box-storage">
      <div class="box-drag-position" id="drag{{number}}" ondragover="dragOver(event)" ondragleave="dragLeave(event)" ondrop="drop(event)">
        Place Box Here
      </div>
    </div>
    {{#item}}
      <div class="card dragBox boxItem" ondragstart="dragStart(event)" draggable="true" id="{{id}}">
        <div class="card-body">
          <h4 class="card-title">{{container.box}}</h4>
          <p class="card-text">
          {{name}}
          </p>
          <a href="#!" class="card-link">Edit Box</a>
          <a href="#!" class="card-link">Delete Box</a>
        </div>
      </div>
      {{/item}}

      {{#drag}}
    <div class="box-drag-position" id="drag{{number}}" ondragover="dragOver(event)" ondragleave="dragLeave(event)" ondrop="drop(event)">
      

    </div>
    {{/drag}}
  </div>
  

</main>
</div>